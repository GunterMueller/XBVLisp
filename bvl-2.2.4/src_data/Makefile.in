aide: traaide
	$(CC) $(CFLAGS) -o aide aide.c
	cat aidvl?.txt > aidvlisp.dat
	if test -n "${AIDE_OPENGL}" ; then \
	    cat aidvl-gl.txt >> aidvlisp.dat ; \
	    cp  GL-Drive-Help.txt GL-Drive-Help.dat ; \
	    ./traaide f GL-Drive-Help ; \
	fi
	cat aidvl-so.txt >> aidvlisp.dat
	./traaide f aidvlisp

